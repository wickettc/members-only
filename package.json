{
    "name": "members-only",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "devstart": "nodemon ./bin/www",
        "serverstart": "DEBUG=members-only:* npm run devstart",
        "winstart": "SET DEBUG=members-only:* & npm run devstart"
    },
    "dependencies": {
        "async": "^3.2.0",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "express": "~4.16.1",
        "express-session": "^1.17.1",
        "express-validator": "^6.9.2",
        "http-errors": "~1.6.3",
        "mongoose": "^5.11.14",
        "morgan": "~1.9.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pug": "2.0.0-beta11"
    },
    "devDependencies": {
        "eslint": "^7.19.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "nodemon": "^2.0.7"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "node": true
        },
        "rules": {
            "no-unused-vars": [
                "warn"
            ],
            "indent": [
                "error",
                4
            ],
            "linebreak-style": [
                "off"
            ],
            "implicit-arrow-linebreak": [
                "off"
            ],
            "function-paren-newline": [
                "off"
            ],
            "consistent-return": [
                "off"
            ],
            "comma-dangle": [
                "off"
            ],
            "operator-linebreak": [
                "off"
            ]
        }
    }
}
